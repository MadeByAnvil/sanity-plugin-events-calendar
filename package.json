{
  "name": "sanity-plugin-events-calendar",
  "version": "1.0.0",
  "description": "A comprehensive events calendar plugin for Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "calendar",
    "events",
    "scheduling"
  ],
  "homepage": "https://github.com/madebyanvil/sanity-plugin-events-calendar#readme",
  "bugs": {
    "url": "https://github.com/madebyanvil/sanity-plugin-events-calendar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/madebyanvil/sanity-plugin-events-calendar.git"
  },
  "license": "MIT",
  "author": "Casey Zumwalt <casey@madebyanvil.com>",
  "sideEffects": false,
  "type": "commonjs",
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false
    }
  },
  "browserslist": "extends @sanity/browserslist-config",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install",
    "test": "echo 'No tests yet. All good!'"
  },
  "dependencies": {
    "@sanity/color": "^3.0.6",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "2.15.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@sanity/pkg-utils": "^7.2.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/react": "^19.1.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanity": "^3.88.3",
    "semantic-release": "^21.0.7",
    "styled-components": "^6.1.18",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.41.0"
  }
}
